{
    "name": "helgesverre/receipt-parser",
    "description": "Use OpenAI to parse structured receipt data from Images, Pdfs and emails.",
    "keywords": [
        "HelgeSverre",
        "laravel",
        "receipt-parser"
    ],
    "homepage": "https://github.com/helgesverre/receipt-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Helge Sverre",
            "email": "helge.sverre@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.281",
        "illuminate/contracts": "^10.0",
        "openai-php/laravel": "^0.7.0",
        "prinsfrank/standards": "^2.1",
        "smalot/pdfparser": "*",
        "spatie/laravel-package-tools": "^1.14.0",
        "symfony/dom-crawler": "^6.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {

            "HelgeSverre\\ReceiptParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelgeSverre\\ReceiptParser\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-receipt-parser --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelgeSverre\\ReceiptParser\\ReceiptParserServiceProvider"
            ],
            "aliases": {
                "ReceiptParser": "HelgeSverre\\ReceiptParser\\Facades\\ReceiptParser"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
